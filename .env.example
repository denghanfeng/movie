REDIS_DB=0

#JWT配置
# 务必改为你自己的字符串
JWT_SECRET=moive223
#token过期时间，单位为秒
JWT_TTL=60
#关闭黑名单
JWT_BLACKLIST_ENABLED=false

#h5页面地址
H5_DOMAIN='http://yzdhf.vaiwan.com'
#云瞻接口地址
YZ_DOMAIN='http://yz.com'
